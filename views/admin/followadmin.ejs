<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin อาสาสมัครสาธารณสุข หมู่ 8</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm" crossorigin="anonymous"></script>

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
    <link rel="stylesheet" href="/public/CSS/admin/headAdmin.css">
    <link rel="stylesheet" href="/public/CSS/admin/followAdmin.css">

</head>
<body>
    <%- include('navbar') %>

    <section class="home">
        <div class="box1-1">
            <div class="story">
                <table class="tables">
                    <thead>
                        <tr>
                            <th>วันที่</th>
                            <th>หัวข้อ</th>
                            <th>สถานที่</th>
                            <th>สถานะ</th>
                            <th>เเก้ไข</th>
                        </tr>
                    </thead>
                    <tbody>

                        <% Object.values(products).forEach(function(items) { %>
                            <tr>
                                <th><%= items.Date %></th>
                                <th><%= items.problem %></th>
                                <th><%= items.address %></th>
                                <th><%= items.statuss %>%</th>
                                <th>
                            
                                    <div>
                                        <form action="/edit" method="post">
                                            <input type="hidden" name="editIDscript" value="<%= items.IDscript %>">
                                            <button type="submit" class="submit-buttonss"><i class="bi bi-pencil-square icons"></i></button>
                                        </form>
                                    </div>
                                    <div>
                                        <button  class="submit-buttons" >
                                            <a href="/deleteFormDatas/<%= items.id %>" onclick="return confirm('คุณต้องการลบข้อมูลหรือไม่')" >
                                                <i class="bi bi-trash3-fill icons"></i>
                                            </a>
                                        </button>
                                    </div>
                                    
                                </th>
                            </tr>
                        <% })%>
                        <% Object.values(assist).forEach(function(itemassist) { %>
                            <tr>
                                <th><%= itemassist.Date %></th>
                                <th><%= itemassist.problem %></th>
                                <th><%= itemassist.address %></th>
                                <th><%= itemassist.statuss %>%</th>
                                <th>
                            
                                    <div>
                                        <form action="/editformassist" method="post">
                                            <input type="hidden" name="editIDscript" value="<%= itemassist.IDscript %>">
                                            <button type="submit" class="submit-buttonss"><i class="bi bi-pencil-square icons"></i></button>
                                        </form>
                                    </div>
                                    <div>
                                        <button  class="submit-buttons" >
                                            <a href="/deleteformassis/<%= itemassist.id %>" onclick="return confirm('คุณต้องการลบข้อมูลหรือไม่')" >
                                                <i class="bi bi-trash3-fill icons"></i>
                                            </a>
                                        </button>
                                    </div>
                                    
                                </th>
                            </tr>
                        <% })%> 
                        <% Object.values(help).forEach(function(itemhelp) { %>
                            <tr>
                                <th><%= itemhelp.Date %></th>
                                <th><%= itemhelp.problem %></th>
                                <th><%= itemhelp.address %></th>
                                <th><%= itemhelp.statuss %>%</th>
                                <th>
                            
                                    <div>
                                        <form action="/editformhelp" method="post">
                                            <input type="hidden" name="editIDscript" value="<%= itemhelp.IDscript %>">
                                            <button type="submit" class="submit-buttonss"><i class="bi bi-pencil-square icons"></i></button>
                                        </form>
                                    </div>
                                    <div>
                                        <button  class="submit-buttons" >
                                            <a href="/deleteFormhelp/<%= itemhelp.id %>" onclick="return confirm('คุณต้องการลบข้อมูลหรือไม่')" >
                                                <i class="bi bi-trash3-fill icons"></i>
                                            </a>
                                        </button>
                                    </div>
                                    
                                </th>
                            </tr>
                        <% })%>  

                        



                        
                    </tbody>

                </table>
            </div>
        </div>
        <div class="box1-2">
            <div class="box2-1">
                <div class="box3-1">
                    <table>
                        <thead>
                            <tr>
                                <th>รายการที่มากที่สุด</th>
                                <th>จำนวน</th>
                                <th>เเก้ไขเเล้ว</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <th>ถนนชำรุด</th>
                                <th>8</th>
                                <th>5</th>
                            </tr>
                            <tr>
                                <th>ถนนชำรุด</th>
                                <th>8</th>
                                <th>5</th>
                            </tr>
                            <tr>
                                <th>ถนนชำรุด</th>
                                <th>8</th>
                                <th>5</th>
                            </tr>
                            
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="box2-2">
                <div class="box4-1">
                
                </div>
            </div>
        </div>
    </section>

    <script src="/public/js/adminJs/navbar.js"></script>
</body>
</html>