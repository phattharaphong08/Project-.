<!-- /* ==================== มกราคม 2566 ==================== */ -->
<div class="wrappers">
    <div class="select-bth">
        <div>
            <span>มกราคม</span>
            <span>2566</span>
        </div>
        <i class="ui uil-angle-down icons"></i>
    </div>
    <div class="contents">
        <ul class="options">
            <% januaryData.sort((a, b) => a.day - b.day).forEach(january => { %>
                <li>
                    <div class="add"><%= january.day %></div>
                    <div class="add"><%= january.eventname %></div>
                    <div class="add"><%= january.eventdiscription %></div>
                    <div class="adds">
                        <a href="/deletejanuary/<%= january.id %>"><i class="bi bi-trash3"></i></a>
                    </div>
                </li>
            <% }); %>
        </ul>
    </div>
</div>
