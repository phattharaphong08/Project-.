<body>
    <%- include('head') %>

    <article>
        <div class="tops">
            <div class="water">
                <% Object.values(homeadmin).forEach(function(item) { %>
                    <div class="middle">
                        <div class="headings">
                            <a><%= item.namehome %></a>
                        </div>
                        <div class="centers">
                            <img src = "/image/imageAdmin/homeimg/<%= item.imagehome %>" alt = "">
                        </div>
        
                        <div class="lowers">
                            <p><%= item.detailhome %></p>
                        </div>
                    </div>
                <% })%>  
            </div>
        </div>
    </article>

